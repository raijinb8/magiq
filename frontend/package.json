{
  "name": "magiq",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:coverage:open": "vitest --coverage && open coverage/index.html",
    "test:coverage:watch": "vitest --coverage --watch",
    "test:coverage:report": "vitest --coverage --run && node scripts/coverage-report.js",
    "test:coverage:threshold": "vitest --coverage --run",
    "test:reporter": "vitest --reporter=verbose --reporter=junit --outputFile=test-results.xml",
    "test:debug": "vitest --inspect-brk --no-coverage",
    "test:ci": "vitest --run --coverage --reporter=junit --outputFile=test-results.xml",
    "ci": "npm run lint && npm run type-check && npm run build && npm run test:ci"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@shadcn/ui": "^0.0.4",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/vite": "^4.1.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "pdfjs-dist": "4.8.69",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-pdf": "^9.2.1",
    "react-router-dom": "^7.5.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.3",
    "tw-animate-css": "^1.2.5",
    "vite-tsconfig-paths": "^5.1.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.1",
    "@vitest/ui": "^3.2.1",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "happy-dom": "^17.6.3",
    "msw": "^2.8.6",
    "prettier": "^3.5.3",
    "supabase": "^2.22.12",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.2.1"
  }
}
